var breadcount
var tunacount
var hamcount
if(breadcount>=2 && hamcount>=1){
displayThisText('I can make a ham sandwich!')
}
else if(breadcount>=2 && tunacount>=1){
displayThisText('can make a tuna sandwich!')
}
else if(breadcount>=2 && tunacount>=1 && hamcount>=1)
{
displayThisText('I can make a royal sandwich!')
}
